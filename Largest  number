#include<stdio.h>
int main()
{
  int a,b,c;
  printf("Enter a value");
  scanf("%d",&a);
  printf("Enter b value");
  scanf("%d",&b);
  printf("Enter c value");
  scanf("%d",&b);
  if(a>b && a>c)
    {
    printf("a is greater ");
   else if (b>c)
     {
     printf("b is greater");
     }
   else
     {
     printf("c is greater");
     }
}


    
